<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sistema Turnos Aduana</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Home Page", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse" style="float:left !important">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Roles", "Index", "Roles")</li>
                    <li role="presentation" class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">
                            Usuario <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Usuarios", "Index", "usuarios")</li>
                            <li>@Html.ActionLink("Tipo Usuarios", "Index", "TipoUsuarios")</li>
                        </ul>
                    </li>
                    <li role="presentation" class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">
                            Contratos <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Contratos", "Index", "Contratos")</li>
                            <li>@Html.ActionLink("Tipo Contratos", "Index", "TipoContratos")</li>
                        </ul>
                    </li>
                    <li role="presentation" class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">
                            Permisos <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Permisos", "Index", "Permisos")</li>
                            <li>@Html.ActionLink("Tipo Permisos", "Index", "TipoPermisos")</li>
                        </ul>
                    </li>
                    <li>@Html.ActionLink("Estados", "Index", "Estados")</li>
                    <li>@Html.ActionLink("Lugares", "Index", "Lugares")</li>
                    <li>@Html.ActionLink("Turnos", "Index", "Turnos")</li>
                    <li>@Html.ActionLink("Turnos Usuarios", "Index", "TurnoUsuarios")</li>
                    <li>@Html.ActionLink("Cambio Turnos", "Index", "CambioTurnos")</li>
                    <li>@Html.ActionLink("Calidad Jurídica", "Index", "CalidadJuridicas")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/css/bootstrap-datepicker.min.css" />
        <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/js/bootstrap-datepicker.min.js"></script>

        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - UNIACC, Proyecto Integración 2</p>
        </footer>
    </div>

 
     
    @RenderSection("scripts", required: false)
</body>
</html>
